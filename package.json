{
  "name": "react-render-actions",
  "version": "1.0.0",
  "description": "Set of JavaScript actions to use during component rendering",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "lib/react-render-actions.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "release": "rimraf lib/* && npm run prettier && npm run lint && npm run build:dev && npm run build",
    "build": "NODE_ENV=production rollup -c",
    "build:dev": "NODE_ENV=development rollup -c",
    "watch": "npm run build -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "lint": "eslint 'src/**/*.ts' --fix",
    "prettier": "prettier --write 'src/**/*.ts'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JB1905/react-render-actions.git"
  },
  "keywords": [
    "React",
    "rendering",
    "loop",
    "async-await",
    "if-else",
    "switch-case",
    "promise"
  ],
  "bugs": {
    "url": "https://github.com/JB1905/react-render-actions/issues"
  },
  "homepage": "https://github.com/JB1905/react-render-actions#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.14",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts}": ["npm run prettier", "npm run lint", "git add"]
  }
}
